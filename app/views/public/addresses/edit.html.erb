<h3>配送先編集</h3>

<%= form_with model: @address, url:address_path, method: :patch do |f| %>
  <!--郵便番号(ハイフンなし)-->
  <label>郵便番号(ハイフンなし)</label>
  <%= f.text_field :postal_code %>
  <!--住所-->
  <label>住所</label>
  <%= f.text_field :address %>
  <!--宛名-->
  <label>宛名</label>
  <%= f.text_field :name %>

  <%= f.submit '変更を保存', class: "btn btn-success" %>
<% end %>
